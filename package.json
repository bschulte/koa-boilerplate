{
  "name": "koa-boilerplate",
  "description": "Koa, Typescript, GraphQL boilerplate",
  "version": "0.0.1",
  "repository": "https://github.com/bschulte/koa-boilerplate",
  "main": "src/main.ts",
  "scripts": {
    "test": "./node_modules/.bin/jest --runInBand",
    "test:watch": "./node_modules/.bin/jest --watch --runInBand",
    "dev": "nodemon",
    "debug": "nodemon --config nodemon-debug.json",
    "start": "./node_modules/.bin/ts-node ./src/app.ts",
    "pm2:start": "pm2 start --interpreter ./node_modules/.bin/ts-node ./src/app.ts --name koa",
    "docker:build": "docker build -t koa .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:pm2": "pm2-runtime --interpreter ./node_modules/.bin/ts-node ./src/app.ts --name koa",
    "docker:shell": "docker exec -it $(docker ps -q) /bin/ash"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^6.1.0",
    "@types/express-jwt": "0.0.40",
    "@types/graphql-type-json": "^0.1.3",
    "@types/http-errors": "^1.6.1",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mjml": "^4.0.2",
    "@types/mocha": "^5.2.6",
    "@types/moment": "^2.13.0",
    "@types/nodemailer": "^4.6.6",
    "@types/owasp-password-strength-test": "^1.3.0",
    "@types/pluralize": "0.0.29",
    "@types/prompts": "^1.2.0",
    "@types/sequelize": "^4.27.38",
    "@types/sinon": "^7.0.9",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "apollo-server-koa": "^2.14.2",
    "apollo-server-testing": "^2.4.8",
    "async-local-storage": "^2.1.0",
    "bcrypt": "^3.0.3",
    "chai": "^4.2.0",
